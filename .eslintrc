{
	"extends": "airbnb",
	"rules": {
		"func-names": ["error", "never"],
		"no-tabs": 0,
		"indent": ["error", "tab"],
		"arrow-parens": 0,
		"no-underscore-dangle": 0,
		"no-unused-vars": 1,
		"semi": 0,
		"comma-dangle": 1,
		"max-len": 1,
		"import/newline-after-import": 0,
		"no-param-reassign": 1,
		"one-var": 1,
		"global-require": 0
	}
}
